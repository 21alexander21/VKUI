    "prop-types": "^15.6.1"
  },
  "scripts": {
    "prepublishOnly": "npm run clear && npm run icons && npm run build",
    "styleguide": "NODE_ENV=development styleguidist server --config=styleguide/config.js",
    "dev": "NODE_ENV=development webpack --config=build/webpack.config.js --watch",
    "styleguide:build": "NODE_ENV=production styleguidist build --config=styleguide/config.js",
    "icons": "rm -rf dist/icons/* && NODE_ENV=production webpack --config=build/webpack.svg-config.js",
    "build": "NODE_ENV=production webpack --config=build/webpack.config.js",
    "clear": "rm -rf dist/*",
    "test": "./node_modules/.bin/eslint . && ./node_modules/.bin/stylelint ./src/**/*.css && ./node_modules/.bin/jest"
  },
  "pre-commit": [
    "test"
  ]
